<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.liteblog.mapper.BlogUserMapper">

    <resultMap id="BaseResultMap" type="com.example.liteblog.domain.BlogUser">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="avatar_url" column="avatar_url" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="gmt_create" column="gmt_create" jdbcType="TIMESTAMP"/>
            <result property="gmt_modified" column="gmt_modified" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,avatar_url,name,
        password,gmt_create,gmt_modified
    </sql>
</mapper>
